alias chs='ls -d ~/dotfiles/cheatSheet/* | xargs -I{} cat {}'
alias h='history'
alias g='grep --color=auto'
alias grep='grep --color=auto'
alias la='ls -la'
alias ll='ls -l'
alias sh='sh -u'
alias t1='tail -n100'
